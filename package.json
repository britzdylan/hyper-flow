{
  "name": "hyper-flow",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#primitives/*": [
      "./fragments/components/primitives/*.js"
    ],
    "#components": [
      "./fragments/components/index.js"
    ],
    "#layouts/*": [
      "./fragments/layouts/*.js"
    ],
    "#pages/*": [
      "./fragments/pages/*.js"
    ],
    "#fragments/*": [
      "./fragments/*.js"
    ]
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.5.1",
    "@adonisjs/eslint-config": "^1.2.1",
    "@adonisjs/prettier-config": "^1.2.1",
    "@adonisjs/tsconfig": "^1.2.1",
    "@japa/assert": "^1.2.4",
    "@japa/plugin-adonisjs": "^3.0.0",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.3.107",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.10",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "pino-pretty": "^10.3.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.2.6"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.0",
    "@adonisjs/core": "^6.8.0",
    "@adonisjs/lucid": "^20.5.1",
    "@adonisjs/session": "^7.4.0",
    "@adonisjs/shield": "^8.1.1",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^2.0.2",
    "@alpinejs/anchor": "^3.13.8",
    "@alpinejs/collapse": "^3.13.8",
    "@alpinejs/focus": "^3.13.8",
    "@vinejs/vine": "^2.0.0",
    "adonisjsx": "^0.2.0",
    "alpinejs": "^3.13.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "htmx.org": "^1.9.11",
    "luxon": "^3.4.4",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "tailwind-merge": "^2.2.2"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config",
  "main": "ace.js",
  "directories": {
    "test": "tests"
  },
  "author": "Dylan Britz",
  "description": ""
}
